{
    "compilerOptions": {
        "moduleResolution": "node",
        "target": "ESNext",
        "module": "CommonJS",
        "downlevelIteration": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitOverride": true,
        "strict": true
    }
}
